<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="code_lovers.assetic.filter.jshint.class">CodeLovers\Assetic\Filter\JsHintFilter</parameter>
    </parameters>

    <services>
        <service id="code_lovers.assetic.filter.jshint" class="%code_lovers.assetic.filter.jshint.class%">
            <tag name="assetic.filter" alias="jshint" />
        </service>
    </services>
</container>